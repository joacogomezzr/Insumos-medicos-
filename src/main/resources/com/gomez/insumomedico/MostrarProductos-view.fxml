<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="fondo" prefHeight="362.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.gomez.insumomedico.controllers.MostrarProductosController">
   <children>
      <ImageView fitHeight="400.0" fitWidth="600.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@imagenes/fond2.jpg" />
         </image>
      </ImageView>
      <TableView fx:id="table" layoutX="145.0" layoutY="25.0" prefHeight="309.0" prefWidth="311.0">
        <columns>
          <TableColumn fx:id="nameColumn" prefWidth="75.0" text="Nombre" />
          <TableColumn fx:id="marcaColumn" prefWidth="75.0" text="Marca" />
            <TableColumn fx:id="precioColumn" prefWidth="75.0" text="Precio" />
            <TableColumn fx:id="cantidadColumn" prefWidth="75.0" text="Cantidad" />
        </columns>
      </TableView>
      <Button fx:id="salirBtn" layoutX="512.0" layoutY="196.0" mnemonicParsing="false" onMouseClicked="#OnMouseClickSalirBtn" prefHeight="29.0" prefWidth="57.0" text="SALIR">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Button>
   </children>
</AnchorPane>
